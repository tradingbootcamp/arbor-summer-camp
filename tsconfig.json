{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@locales/*": [
        "locales/*"
      ],
      "@i18n/*": [
        "i18n/*"
      ],
      "@config/*": [
        "config/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}